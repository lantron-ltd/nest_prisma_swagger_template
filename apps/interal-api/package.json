    "@nestjs/common": "^9.3.2",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.3.2",
    "@nestjs/microservices": "^9.3.7",
    "@nestjs/platform-express": "^9.3.2",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/terminus": "^9.2.0",
    "@nestjs/throttler": "^4.0.0",
    "@prisma/client": "^4.10.0",
    "apollo-server-core": "^3.11.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "js-yaml": "^4.1.0",
    "nestjs-redis": "^1.3.3",
    "prisma": "^4.10.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.19",
    "add": "^2.0.6",
    "class-validator": "^0.14.0",
    "source-map-support": "^0.5.21",
    "yarn": "^1.22.19"
  },
  "engines": {
    "node": ">=16.x"
  },
